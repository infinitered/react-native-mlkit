{
  "private": true,
  "scripts": {
    "build": "turbo run build --filter=!docusaurus",
    "build:docs": "turbo run build --filter=docusaurus",
    "docs": "yarn workspace docusaurus start",
    "test": "turbo run test",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "setup": "./scripts/setup.sh",
    "version": "changeset version && yarn install --mode update-lockfile",
    "ci:build": "turbo run build",
    "ci:publish": "changeset publish",
    "ci:log": "[ ! -z \"$GITHUB_RUN_ID\" ] && echo 'Successfully published changeset: $GITHUB_RUN_ID' > ./yarn-release.log",
    "ci:push-tags": "git push --follow-tags",
    "release": "node scripts/release.mjs"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@changesets/get-release-plan": "^3.0.17",
    "@turbo/gen": "^1.9.7",
    "@types/jest": "^29.5.5",
    "eslint": "^8.0.0",
    "execa": "^8.0.1",
    "jest": "^29.7.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "^2.5.1",
    "ts-jest": "^29.1.1",
    "turbo": "latest",
    "typescript": "^5.2.2"
  },
  "name": "react-native-mlkit",
  "workspaces": [
    "apps/*",
    "modules/*",
    "packages/*"
  ],
  "packageManager": "yarn@3.6.1",
  "repository": "https://www.github.com/infinitered/react-native-mlkit"
}
